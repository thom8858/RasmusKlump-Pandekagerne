{
    "manifest_version": 2,
    "name": "Rasmus Pandekage",
    "version": "0.1",
    "icons": {
        "64": "icons/pandekage_icon_64.png",
        "128": "icons/pandekage_icon_128.png",
        "256": "icons/pandekage_icon_256.png"
    },
    "browser_action": {
        "default_icon": "icons/pandekage_icon_256.png"
    },
    "content_scripts": [
        {
            "matches": [
        "<all_urls>"
      ],
            "js": ["palukage.js"]
    }
  ]
}
